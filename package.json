{
  "name": "quiz-cache",
  "displayName": "Quiz cache",
  "version": "0.0.1",
  "description": "QuizCache is a browser extension engineered to solve the \"regressive error\" problem in multi-attempt assessments. By acting as a local caching layer between the user and the Canvas LMS, it persists the state of verified correct answers across different quiz sessions.",
  "author": "Zhuen Zhong",
  "scripts": {
    "dev": "PLASMO_SRC_PATH=. plasmo dev",
    "build": "PLASMO_SRC_PATH=. plasmo build",
    "package": "PLASMO_SRC_PATH=. plasmo package"
  },
  "dependencies": {
    "lucide-react": "^0.562.0",
    "plasmo": "0.90.5",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "autoprefixer": "^10.4.23",
    "postcss": "^8.5.6",
    "prettier": "3.2.4",
    "tailwindcss": "^3.4.19",
    "typescript": "5.3.3"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ]
  }
}
